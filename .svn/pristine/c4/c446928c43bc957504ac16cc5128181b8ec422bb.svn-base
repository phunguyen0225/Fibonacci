package app;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.*;

public class FibonacciTest {

  @Test
  public void canary() {
    assertTrue(true);
  }

  @Test
  public void imperativeFibAtPosition0() { //Feedback: let's merge these tests into one test
    assertEquals(1, FibonacciImperative.fibonacciAtPosition(0));
  }

  @Test
  public void imperativeFibAtPosition1() {
    assertEquals(1, FibonacciImperative.fibonacciAtPosition(1));
  }

  @Test
  public void imperativeFibAtPosition2() {
    assertEquals(2, FibonacciImperative.fibonacciAtPosition(2));
  }

  @Test
  public void imperativeFibAtPosition5() {
    assertEquals(8, FibonacciImperative.fibonacciAtPosition(5));
  }

  @Test
  public void imperativeFibAtPosition10() {
    assertEquals(89, FibonacciImperative.fibonacciAtPosition(10));
  }

  @Test
  public void imperativeFibAtPosition50() {
    assertEquals(20365011074L, FibonacciImperative.fibonacciAtPosition(50));
  }

